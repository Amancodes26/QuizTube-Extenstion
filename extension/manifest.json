{
  "manifest_version": 3,
  "name": "YouTube Quiz Generator",
  "version": "1.0",
  "description": "Generate quizzes from YouTube videos",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
